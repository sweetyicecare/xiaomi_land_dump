	hexagon-smp2p {
		compatible = "qcom,smp2p";
		qcom,smem = <0x1b3 0x1ac>;
		interrupts = <0x0 0x1b 0x1>;
		qcom,ipc = <0x54 0x8 0xe>;
		qcom,local-pid = <0x0>;
		qcom,remote-pid = <0x1>;

		master-kernel {
			qcom,entry-name = "master-kernel";
			#qcom,smem-state-cells = <0x1>;
		};

		slave-kernel {
			qcom,entry-name = "slave-kernel";
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};
	};

	wcnss-smp2p {
		compatible = "qcom,smp2p";
		qcom,smem = <0x1c3 0x1af>;
		interrupts = <0x0 0x8f 0x1>;
		qcom,ipc = <0x54 0x8 0x12>;
		qcom,local-pid = <0x0>;
		qcom,remote-pid = <0x4>;

		master-kernel {
			qcom,entry-name = "master-kernel";
			#qcom,smem-state-cells = <0x1>;
		};

		slave-kernel {
			qcom,entry-name = "slave-kernel";
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};
	};

	smsm {
		compatible = "qcom,smsm";
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		qcom,ipc-1 = <0x54 0x8 0xd>;
		qcom,ipc-3 = <0x54 0x8 0x13>;

		apps@0 {
			reg = <0x0>;
			#qcom,smem-state-cells = <0x1>;
		};

		hexagon@1 {
			reg = <0x1>;
			interrupts = <0x0 0x1a 0x1>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};

		wcnss@6 {
			reg = <0x6>;
			interrupts = <0x0 0x90 0x1>;
			interrupt-controller;
			#interrupt-cells = <0x2>;
		};
	};

	usb2513 {
		compatible = "smsc,usb3503";
		reset-gpios = <0x41 0x3 0x1>;
		initial-mode = <0x1>;
	};

	usb-id {
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <0x1f 0x79 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0x56>;
		linux,phandle = <0x3d>;
		phandle = <0x3d>;
	};

	hdmi-out {
		compatible = "hdmi-connector";
		type = [61 00];

		port {

			endpoint {
				remote-endpoint = <0x57>;
				linux,phandle = <0x27>;
				phandle = <0x27>;
			};
		};
	};
};
