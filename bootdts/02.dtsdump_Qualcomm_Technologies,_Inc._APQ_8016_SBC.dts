			};

			spi6_sleep {
				linux,phandle = <0x1a>;
				phandle = <0x1a>;

				pinmux {
					function = "gpio";
					pins = "gpio20", "gpio21", "gpio22", "gpio23";
				};

				pinconf {
					pins = "gpio20", "gpio21", "gpio22", "gpio23";
					drive-strength = <0x2>;
					bias-pull-down;
				};
			};

			i2c2_default {
				linux,phandle = <0x1b>;
				phandle = <0x1b>;

				pinmux {
					function = "blsp_i2c2";
					pins = "gpio6", "gpio7";
				};

				pinconf {
					pins = "gpio6", "gpio7";
					drive-strength = <0x10>;
					bias-disable = <0x0>;
				};
			};

			i2c2_sleep {
