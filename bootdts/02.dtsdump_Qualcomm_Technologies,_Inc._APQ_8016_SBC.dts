				phandle = <0x29>;

				pinmux {
					function = "gpio";
					pins = "gpio22", "gpio23";
				};

				pinconf {
					pins = "gpio22", "gpio23";
					drive-strength = <0x2>;
					bias-disable = <0x0>;
				};
			};

			sdhc2_cd_pin {

				cd_on {
					linux,phandle = <0x37>;
					phandle = <0x37>;

					pinmux {
						function = "gpio";
						pins = "gpio38";
					};

					pinconf {
						pins = "gpio38";
						drive-strength = <0x2>;
						bias-pull-up;
					};
				};

				cd_off {
					linux,phandle = <0x3b>;
					phandle = <0x3b>;

					pinmux {
						function = "gpio";
						pins = "gpio38";
					};

					pinconf {
