					pins = "gpio6", "gpio7";
				};

				pinconf {
					pins = "gpio6", "gpio7";
					drive-strength = <0x2>;
					bias-disable = <0x0>;
				};
			};

			i2c4_default {
				linux,phandle = <0x1d>;
				phandle = <0x1d>;

